        $('.PDP_ziperror').show();
      	var PDP_zip_code = $('.dtl_input').val(); //110033
          var reg = /^[0-9]+$/;
          if (PDP_zip_code == '') {
              errorMessage = "*Zipcode required!";
            	$('.PDP_ziperror').text(errorMessage);


          } else if ((PDP_zip_code.length) < 6 || (PDP_zip_code.length) > 6) {
              errorMessage = "*zipcode should only be 6 digits";
            	$('.PDP_ziperror').text(errorMessage);

          } else if (!reg.test(PDP_zip_code)) {
              errorMessage = "*zipcode should be numbers only";
            	$('.PDP_ziperror').text(errorMessage);

          } else {
              errorMessage = "Loading...";
            	$('.PDP_ziperror').css('color','#333333');
            	$('.PDP_ziperror').text(errorMessage);
              $('.h_location p').text(PDP_zip_code);
              $('.h_location p').attr('data-location', PDP_zip_code);
            	getDeliveyInfo();
        }
      });

      // Cheack other pincode
      $('.dtl_pin_box button').click(function(){
      	$('.dtl_pin_box').hide();
        $('.dtl_input').show();
      	$('.dtl_btn').show();
        $('.dtl_input').val("");
      })




  })
