<script>
    function getDeliveyInfo(){
  	var PDP_selected_sku = $('[name="id"]').val();
    var PDP_selected_qty = 1;
    var PDP_request_no = 2026566975550;
  	var PDP_zip_code = $('.dtl_input').val(); //110033
    var dtl_per = 'No';
    var dtl_currenttime = new Date();
      dtl_currenttime = dtl_currenttime.getHours();

      
      	dtl_per = "No";
      

  	$('.PDP_dtl_dateOuter').hide();
    //$('.PDP_dtl_locOuter').hide();
    $('.PDP_ziperror').show();

  	var settings = {
  		//"url": "https://logistic.headsupfortails.com/polls/api/v1/order_delivery_location/",
          //"url": "https://huft-qa.headsupfortails.com/polls/api/v1/order_delivery_location/",
          //"url": "https://preprodapi.headsupfortails.com/v1/order/get-nearest-store",
          "url": "https://api.headsupfortails.com/v1/order/get-nearest-store",
  		"method": "POST",
  		"timeout": 0,
  		"headers": {
  			"Content-Type": "application/json"
  		},
  		"data": JSON.stringify({
  			"request_no": PDP_request_no,
  			//"API_Source": "Product,",
  			"pin_code": PDP_zip_code,
  			//"state": "",
  			//"api_source": "Product",
  			"line_items": [{
  				"variant_id": PDP_selected_sku,
  				"quantity": PDP_selected_qty,
                "is_personalised": dtl_per
  			}]
  		}),
  	};
    //console.log(settings);
  	$.ajax(settings).done(function(response) {
  		$('.PDP_ziperror').hide();
  		$('.PDP_dtl_response').show();
  		console.log(response);

  		//var dtlStatus = response.result.status;
        var dtlStatus = response.code;
  		//var dtlResponseLnt = response.result.response.length; && dtlResponseLnt > 0

  		 if (dtlStatus == 200) {

  		  //var dtl_date = response.result.response[0].edt;
            var dtl_date = response.data.response[0].min_edt;
            var dtl_max_date = response.data.response[0].max_edt;
            dtl_max_date = new Date(dtl_max_date).toDateString();

    		var dtl_pickup = response.data.response[0].self_pickup;

            // date format
            var dtl_date = new Date(dtl_date).toDateString();
            var dtlTodayDate = new Date().toDateString();
            const dtlTomorrowDate = new Date()
  		    dtlTomorrowDate.setDate(new Date().getDate() + 1)

            var dtl2days = new Date();
            dtl2days.setDate(new Date().getDate() + 2);
            dtl2days = dtl2days.toDateString().split(' ').slice(0,3).join(' ');

             var dtl4days = new Date();
            dtl4days.setDate(new Date().getDate() + 4);
            dtl4days = dtl4days.toDateString().split(' ').slice(0,3).join(' ');

            if(dtl_date === dtlTodayDate){
              var dtl_final_date = "Today";
              //express msg 
              $(".PDP_info_express").show();
              $(".PDP_dtl_express").show();
              var expressHTML =` <div class="EDT_Express_Delivery">
                <img src="https://cdn.shopify.com/s/files/1/0086/0795/7054/files/day_dash_1.svg?v=1750940726">
                <span>Get it <i>today</i></span>
              </div>`;
              
              $(".PDP_info_express").html(expressHTML);
              $(".PDP_dtl_express").html(expressHTML);
              // express msg ends

              
            }else if( dtl_date === dtlTomorrowDate.toDateString()){
              //var dtl_final_date = "Tomorrow";
              var dtl_final_date = "Tomorrow" + ' ('+ dtl_date +' - By 9PM)';

              //express msg 
              $(".PDP_info_express").show();
              $(".PDP_dtl_express").show();
             var expressHTML =` <div class="EDT_Express_Delivery">
                <img src="https://cdn.shopify.com/s/files/1/0086/0795/7054/files/Group_1410089452.svg?v=1750945431">
                <span>Get it by <i>tomorrow</i></span>
              </div>`;
              
              $(".PDP_info_express").html(expressHTML);
              $(".PDP_dtl_express").html(expressHTML);
              // express msg ends 
                
            }else{
              //var dtl_final_date = dtl_date;
              $(".PDP_info_express").hide();
              $(".PDP_dtl_express").hide();
              var dtl_final_date = dtl_max_date;
            }

             $('.PDP_dtl_dateOuter').show();

             $('.PDP_dtl_date').html(dtl_final_date);
             

              var dtl_storeDetails = response.data.response[0].items.length;
              if(dtl_storeDetails > 0){
                var dtl_storeName = response.data.response[0].items[0].businessName;
                var dtl_storeImage = response.data.response[0].items[0].imageUrl;
                var dtl_storePhone = response.data.response[0].items[0].primaryPhone;
                var dtl_storeMapLink = response.data.response[0].items[0].map_link;
                var dtl_storeAddress = response.data.response[0].items[0].address;
                var addressString = dtl_storeAddress.line1 + ', ' 
                  + dtl_storeAddress.line2 + ', ' 
                  + dtl_storeAddress.locality + ', ' 
                  + dtl_storeAddress.administrativeArea + ', ' 
                  + dtl_storeAddress.countryRegion + ', ' 
                  + dtl_storeAddress.postalCode;
                var dtl_storeTimings = response.data.response[0].items[0].timings;

                $(".dtl_storeName").text(dtl_storeName);
                $(".dtl_storeImage").attr('src', dtl_storeImage);
                $(".dtl_storeAddress").text(addressString);
                $(".dtl_storePhone").attr('href','tel:'+dtl_storePhone+'');
                $(".dtl_storeMapLink").attr('href',dtl_storeMapLink);
                $(".dtl_storeTimings").text(dtl_storeTimings);
                $(".pdp_store_widget").show();
                
              }else{
                //hide store widget
                $(".pdp_store_widget").hide();
              }


  		  window.sessionStorage.setItem("DTL_pin", PDP_zip_code);

  		}else {
  		   $('.PDP_dtl_dateOuter').show();
  		   $('.PDP_dtl_date').html("Pincode not serviceable");
  		}


  	});

  }

    $('document').ready(function() {
      var PDP_zip_code = $('.dtl_input').val(); //110033
      


