$(document).ready(function () {
  const storageKey = "pageHistory";
  const currentPage = window.location.href;

  let pageHistory = JSON.parse(localStorage.getItem(storageKey)) || [];

  if (!pageHistory.length || pageHistory[pageHistory.length - 1] !== currentPage) {
    pageHistory.push(currentPage);
    localStorage.setItem(storageKey, JSON.stringify(pageHistory));
  }

  $("#gobackhistory").on("click", function (e) {
    e.preventDefault(); // Prevent default action

    pageHistory.pop();
    const previousPage = pageHistory.pop();

    if (previousPage) {
      localStorage.setItem(storageKey, JSON.stringify(pageHistory));
      window.location.href = previousPage; // Navigate to the previous page
    } else {
      window.location.href = "/";
    }
  });
});


         
