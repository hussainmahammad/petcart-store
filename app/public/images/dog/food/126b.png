<script>
$(document).ready(function() {
  $(document).on('click', '.view-details-btn', function() {

$('body').css('overflow', 'hidden');

    //add loader
    $(this).find('.loading-overlay__spinner').removeClass('hidden');
        setTimeout(function () {
        $('.loading-overlay__spinner').addClass('hidden');
    },1000);
    
    var productHandle = $(this).data('handle'); // get the product handle
    
    //product usp include in popup
    var parentcardusp = $(this).parent().parent().find('.hw_produt_tag').html();
    $('.plp_popusp').html(parentcardusp);

    //product offers
     var parent_pdp_offer = $(this).parent().find('.hw_plp_offersList ul').html();
    if(parent_pdp_offer && $.trim(parent_pdp_offer) !== ""){
      $('#popup-offers').show();
    $('#popup-offers ul').html(parent_pdp_offer);
    }
    
    // for size chart
    var prodcy_sizechart = $(this).parent().data('sizechart');

        if(prodcy_sizechart && $.trim(prodcy_sizechart) !== ""){   
     $('.pop_pdp_sizecart').show();
     $('.popsdp_prod_sizechart img').attr('src', prodcy_sizechart);
     }

    // for wishlist
    var prodcy_product_id = $(this).parent().data('productid');
    
    $('#product-details .wishlist_icon').attr('data-wishlistid', prodcy_product_id);
    
  var recom_collectionId = $(this).parent().data('collectionid');
      var recom_collectiontitle = $(this).parent().data('collectiontitle');
    
   // FOr reviews
    let pop_review_list = $(this).parent().siblings('.hw_product_review_list').text();   
    var prodcy_reviewCount = $(this).parent().siblings('.hw_product_review_list').data('reviewcount');
    if(pop_review_list && $.trim(pop_review_list) !== ""){
      $('.popup_review').show();
       
    $('.popup_review p').html(pop_review_list);
   $('.popup_review a').attr('href', `https://headsupfortails.com/products/${productHandle}#pdpfadinid`);
    $('.popup_review a span').html(prodcy_reviewCount);
    }
    else{
      $('.popup_review').hide();
    }
   // end review


    $.ajax({
      url: '/products/' + productHandle + '.js',
      method: 'GET',
      dataType: 'json',
      success: function(productData) {
         $('#popuo-vendor a').attr('href', `https://headsupfortails.com/products/${productHandle}`);
           $('#popuo-vendor h3').text(productData.vendor);
        
        $('#popup-title h2').text(productData.title);
         $('#popup-title a').attr('href', `https://headsupfortails.com/products/${productHandle}`);

        
        // popup images
        var pop_pro_img ="";
          productData.images.forEach(function(index) {

            let imgWsiZe = index.split('?')[0];
            //console.log(um);
            let popnewSrc = imgWsiZe.replace(/\.(jpg|png|webp|gif)$/, '_600x600.$1');
              pop_pro_img +=  `<img src="${popnewSrc}">`
          });
            //console.log(pop_pro_img);
        $('.pop_product_img').html(pop_pro_img);
        $('#popup-price').text('₹' + (productData.price / 100).toFixed(2));
          
         $('#popup-price').attr('date-popprice', (productData.price / 100).toFixed(2));

         if (productData.compare_at_price) {
            $('#popup-price').text('₹' + (productData.price / 100).toFixed(2));
            $('#popup-price').attr('date-popprice', (productData.price / 100).toFixed(2));
             }
              else{
                   $('#popup-price').text('MRP: ₹' + (productData.price / 100).toFixed(2));
              } 
        if (productData.compare_at_price) {
          var discountPercentage = (((productData.compare_at_price - productData.price) / productData.compare_at_price) * 100).toFixed();
          $('#popup-price').append(`<div class="pro_pop_comp"> <span class="compare-price">MRP: <s>$₹{(productData.compare_at_price / 100).toFixed()}</s></span> <span class="discount">(${discountPercentage}% off)</span></div>`);
        }

        var variantOptions = {};
        productData.options.forEach(function(option) {
          variantOptions[option.name] = [];
        });

        productData.variants.forEach(function(variant) {
          variant.options.forEach(function(optionValue, index) {
            var optionName = productData.options[index].name;
            if (!variantOptions[optionName].some(function(option) {
              return option.value === optionValue;
            })) {
              variantOptions[optionName].push({
                value: optionValue,
                variantId: variant.id,
                price: variant.price,
                compareAtPrice: variant.compare_at_price,
                available: variant.available
              });
            }
          });
        });

        var variantsHTML = '';
        var primaryOption = productData.options[0].name;
        var secondaryOption = productData.options[1] ? productData.options[1].name : null;

        for (var optionType in variantOptions) {
          variantsHTML += `<div class="pop_single_vraint"><label class="${optionType.replace(' ','-').toLowerCase()}">${optionType}:</label>
          <div class="popvarient" id="variant-options-${optionType}">`;
          variantOptions[optionType].forEach(function(option, index) {
            var uniqueId = `variant-${option.variantId}-${optionType.replace(/\s/g, '-')}`;
            var discountHTML = '';
            if (!secondaryOption || (optionType === secondaryOption && productData.options.length === 2)) {
              if (option.compareAtPrice > option.price) {
                discountHTML = `<div class="pro_pop_comp"><span class="variant-discount" data-price="${option.price}" data-compare="${option.compareAtPrice}"> ${(((option.compareAtPrice - option.price) / option.compareAtPrice) * 100).toFixed()}% </span></div>`;
              }
            }
            variantsHTML += `
              <div class="pop_variant">
                <input type="radio" id="${uniqueId}" data-variant="${option.available}" 
                name="variant-${optionType}" value="${option.value}" data-variant-id="${option.variantId}"
                ${option.available ? (index === 0 ? 'checked' : '') : 'disabled'}>
                <label class="${option.available ? '' : 'unavailable'}" for="${uniqueId}">${option.value} </label>
                ${discountHTML}
              </div>`;
          });
          variantsHTML += '</div></div>';
        }

        if (productData.variants.length === 1) {
          $('#selected-variant-id').val(productData.variants[0].id);
          $('#popup-variants').hide();
        } else {
          $('#popup-variants').html(variantsHTML).show();
        }

        $('#popup-add-to-cart').html(`
        <product-form class="product-forma">
          <form action="/cart/add" method="post" id="product-form" data-type="add-to-cart-form" class="form no_bold_options">
            <input type="hidden" name="id" id="selected-variant-id" value="${productData.variants[0].id}">
             <span class="CLS_GAdata" data-coptions="${productData.options.length}" data-chandle="${productHandle}"
                      data-cid="${recom_collectionId}" data-ctitle="${recom_collectiontitle}" data-cqty="1"></span>
            <button type="submit" id="add-to-cart-btn" 
            class="product-form__submit button button--full-width button--primary clsVartAddToCart">Add
              <div class="loading-overlay__spinner hidden">
                <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
                  <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                </svg>
              </div></button>
              <input type="hidden" name="product-id" value="${prodcy_product_id}">
          </form>
        </product-form>
        `);
 $('#product-popup').show();
        $('.popup-content').css('transform', 'translateY(0%)');
        function isMatchingVariant(variant, selectedOptions, productOptions) {
          for (var i = 0; i < variant.options.length; i++) {
            var optionName = productOptions[i].name;
            if (selectedOptions[optionName] !== variant.options[i]) {
              return false;
            }
          }
          return true;
        }

        function updateVariantSelection() {
          var selectedOptions = {};
          $('input[name^="variant-"]:checked').each(function() {
            var optionType = $(this).attr('name').replace('variant-', '');
            selectedOptions[optionType] = $(this).val();
          });

          var matchingVariant = productData.variants.find(function(variant) {
            return isMatchingVariant(variant, selectedOptions, productData.options);
          });

          if (matchingVariant) {
             if (matchingVariant.compare_at_price) {
            $('#popup-price').text('₹' + (matchingVariant.price / 100).toFixed(2));
             $('#popup-price').attr('date-popprice', (matchingVariant.price / 100).toFixed(2));
             }
              else{
                   $('#popup-price').text('MRP: ₹' + (matchingVariant.price / 100).toFixed(2));
              $('#popup-price').attr('date-popprice', (matchingVariant.price / 100).toFixed(2));
              } 
            if (matchingVariant.compare_at_price) {
              var discountPercentage = (((matchingVariant.compare_at_price - matchingVariant.price) / matchingVariant.compare_at_price) * 100).toFixed();
              $('#popup-price').append(` <div class="pro_pop_comp"><span class="compare-price">MRP: <s>₹${(matchingVariant.compare_at_price / 100).toFixed()}</s></span> <span class="discount">(${discountPercentage}% off)</span></div>`);
            }
            $('#selected-variant-id').val(matchingVariant.id);
          }

          // Update secondary discounts based on first variant
          if (secondaryOption) {
            var firstValue = selectedOptions[primaryOption];
            $('input[name^="variant-' + secondaryOption + '"]').each(function() {
              var secondValue = $(this).val();
              var matchingVariant = productData.variants.find(function(v) {
                return v.options[0] === firstValue && v.options[1] === secondValue;
              });
              $(this).parent('.pop_variant').find('.variant-discount').remove();
              if (matchingVariant && matchingVariant.compare_at_price > matchingVariant.price) {
                var percent = (((matchingVariant.compare_at_price - matchingVariant.price) / matchingVariant.compare_at_price) * 100).toFixed();
                $(this).parent('.pop_variant').append(`<span class="variant-discount"> ${percent}% </span>`);
              }
            });

            // Update availability for secondary options
            $('input[name^="variant-' + secondaryOption + '"]').each(function() {
              var optionValue = $(this).val();
              var isAvailable = productData.variants.some(function(variant) {
                return variant.options[0] === selectedOptions[primaryOption] &&
                       variant.options[1] === optionValue &&
                       variant.available;
              });
              $(this).prop('disabled', !isAvailable);
              $(this).next('label').toggleClass('unavailable', !isAvailable);
              if (!isAvailable) {
                $(this).prop('checked', false);
              }
            });

            // Update availability for primary options
            $('input[name^="variant-' + primaryOption + '"]').each(function() {
              var optionValue = $(this).val();
              var isPrimaryAvailable = productData.variants.some(function(variant) {
                return variant.options[0] === optionValue &&
                       variant.available &&
                       $('input[name^="variant-' + secondaryOption + '"][value="' + variant.options[1] + '"]').prop('disabled') === false;
              });
              $(this).prop('disabled', !isPrimaryAvailable);
              $(this).next('label').toggleClass('unavailable', !isPrimaryAvailable);
              if (!isPrimaryAvailable) {
                $(this).prop('checked', false);
              }
            });
          }
        }

        function selectFirstAvailableCombination() {
          productData.variants.some(function(variant) {
            if (variant.available) {
              variant.options.forEach(function(optionValue, index) {
                var optionType = productData.options[index].name;
                $(`input[name="variant-${optionType}"][value="${optionValue}"]`).prop('checked', true);
              });
              updateVariantSelection();
              return true;
            }
            return false;
          });
        }

        selectFirstAvailableCombination();
        $('input[name^="variant-"]').on('change', updateVariantSelection);
      },
      error: function() {
        window.location.href = "https://headsupfortails.com/products/"+productHandle;
      
      }
    });
  });



