    <script>
      $('document').ready(function(){
        $("#whatsapp-message").click(function(){

          if($(this).hasClass('ct_icon')){
            $('body').removeClass('ct_fixed');
            $("#whatsapp-message").removeClass('ct_icon');
        	$('.ct_overlay').hide();
          	$('.ct_popup').hide();
          }else{
