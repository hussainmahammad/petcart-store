function sdpGetCartDetails(){
  
  let dtlCartItems = [];
  let dtl_vart_id;
  let dtl_vart_qty;
  let dtl_per = 'No';
  var dtl_arry = {};
  let dtl_request_no = 1234;
  let dtl_zipcode = "";
  if (sessionStorage.getItem("DTL_pin") != null) {
    dtl_zipcode = sessionStorage.getItem("DTL_pin");
  }
 
  if(dtl_zipcode.length == 6){
    // content update in widget 
    $(".sdp_cartDeliveryHeading").html("Deliver to <b>"+dtl_zipcode+"</b>");
    $(".sdp_cartPinCodeWidget input").val(dtl_zipcode);
    //$(".sdp_cartPinCodeWidget button").html("Change Pincode");
    
      $.ajax({
    type: 'GET',
    url: '/cart.js',
    dataType: 'json',
    success: function (cartResponse) {
      //console.log(cartResponse);
      if(cartResponse.items.length >= 1){
        $.each(cartResponse.items, function( key, value ) {
          
          if(value.gift_card != true){
            
            dtl_vart_id = value.id;
            dtl_vart_qty = value.quantity;
        
            if((value.title.indexOf('Personalised') != -1) || (value.title.indexOf('Made to Order') != -1)){
              dtl_per = "Yes"; 
            }else{
              dtl_per = "No";
            }
            dtl_arry = {
             "variant_id": dtl_vart_id,
              "quantity": dtl_vart_qty,
              "is_personalised":dtl_per
            }
            dtlCartItems.push(dtl_arry);
            
          }
          
        })
      }  

      // ajax code 
        var settings = {
    		"url": "https://logistic.headsupfortails.com/polls/api/v1/order_delivery_location/",
    		"method": "POST",
    		"timeout": 0,
    		"headers": {
    			"Content-Type": "application/json"
    		},
    		"data": JSON.stringify({
    			"request_no": dtl_request_no,
    			"API_Source": "Cart,",
    			"pin_code": dtl_zipcode,
    			"state": "",
    			"api_source": "Cart",
    			"line_items": dtlCartItems
    		}),
    	};
     
      
      
        $.ajax(settings).done(function(response) {
    	
          	var dtlStatus = response.result.status;
    		//var dtlResponseLnt = response.result.response.length;
          	
    
    		 if (dtlStatus == 200) {
               var dtl_response_arr = response.result.response; 
               $(".sdp_CPE").hide();
                $(".sdp_cartPinCodeOverlay").hide();
                $(".sdp_cartPinCodeForm").hide();
               
               //var dtl_cart_html;
               $.each(dtl_response_arr, function( key, value ) {
                 
                //console.log('caste: ' + value.max_edt + ' | id: ' +value.variant_id);
                  var dtl_date = value.min_edt; 
                  var dtl_pickup = value.self_pickup;
                  var dtl_Storeloc = value.items[0].loc_code;
                 var dtl_spilt_sku = value.items.length;
                 var DTL_days_range = value.items[0].edt_days;
                 var dtl_max_date = value.max_edt;
                 
                 var dtl_wh_option = value.items[0].within_warehouse_region;
                 
                 var dtl_text1= "Delivered by";
              		dtl_max_date = new Date(dtl_max_date).toDateString();
                 
                  // date format 
                  var dtl_date = new Date(dtl_date).toDateString();
                  var dtlTodayDate = new Date().toDateString();
                  const dtlTomorrowDate = new Date()
                  dtlTomorrowDate.setDate(new Date().getDate() + 1);
                  
                  
                  var dtl2days = new Date();
                  dtl2days.setDate(new Date().getDate() + 2);
                  dtl2days = dtl2days.toDateString().split(' ').slice(0,3).join(' ');
    
                   var dtl4days = new Date();
                  dtl4days.setDate(new Date().getDate() + 4);
                  dtl4days = dtl4days.toDateString().split(' ').slice(0,3).join(' ');
    
    
                  if(dtl_date === dtlTodayDate){
                     var dtl_final_date = "Today"; 
                  }else if( dtl_date === dtlTomorrowDate.toDateString()){
                      var dtl_final_date = "Tomorrow";
                  }else{
                      //var dtl_final_date = dtl_date;
                   var dtl_final_date = dtl_max_date;
                  }
                 
                	 if(dtl_date != dtl_max_date){
             	 		dtl_final_date = "Between "+dtl_final_date+" to "+dtl_max_date;
                       dtl_text1 = "Delivery";
               		}
    
                     if(dtl_pickup == "yes") { //new code
                     	 var dtl_cart_html = '<span>'+dtl_text1+'</span> <span>'+dtl_final_date+'</span>';
                     }else{ //new code 
                       
                      //dtl_final_date = "Between "+dtl_final_date+" to "+dtl_max_date;
                          var dtl_cart_html = '<span>Delivered by</span> <span>'+dtl_final_date+'</span>';
                       
                     }
                 
                  // if(dtl_pickup == "yes") {
                   //  dtl_cart_html += '<tr class="dtl_cart_item1"><td colspan="4">Available for Pickup at '+dtl_Storeloc+'</td></tr>';
                  // }
                 
                 //$(dtl_cart_html).insertAfter( '[data-variant-id="'+value.variant_id+'"]' );
                 $('.sdp_cartEDT').show();
                 $('[sdp-data-variant-id="'+value.variant_id+'"]').html(dtl_cart_html);
                 
                 
              });
               
            }else{
               //pincode not serviceable 
                $('.sdp_CPE').show();
    		    $('.sdp_CPE').html("Pincode not serviceable"); 
                $(".sdp_cartEDT").hide();
                $(".sdp_cartPinCodeWidget span").html("Check delivery time");
                $(".sdp_cartPinCodeWidget button").html("Check");
            }
          
    	});

      // ajax code ends 
      
    }
  })
  }

}

