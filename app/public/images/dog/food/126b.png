//popup close code
  $('.close-btn, .pro_popup_outeroverlay').on('click', function() {
    $('body').css('overflow', 'auto');//add overflow hidden to body
    $('.pop_product_img img').attr('src', '');//empty image src from popup
    $('.plp_popusp').html(' '); //remove usp from popup
     $('#product-details .wishlist_icon').removeClass('WHL_removeBtn'); // wishlist icon replaced
        $('#product-details .wishlist_icon').addClass('WHL_addBtn');  // wishlist icon replaced
     $('.popup-content').css('transform', 'translateY(100%)'); // add transition in popup
    $('#product-popup').hide();
      $('#popup-offers').hide();
    $('.pop_pdp_sizecart').hide();
  });
});
</script>


